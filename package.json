{
  "name": "appwrite-cms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev-win": "nuxt",
    "dev": "npm run fix-appwrite && nuxt",
    "build": "npm run fix-appwrite && nuxt generate",
    "start": "npm run fix-appwrite nuxt start",
    "generate": "npm run fix-appwrite && nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "fix-appwrite": "cp _temp_appwrite_fix.txt node_modules/node-appwrite/lib/query.js && cp _temp_appwrite_fix2.txt node_modules/node-appwrite/lib/client.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@tiptap/starter-kit": "^2.0.0-beta.171",
    "@tiptap/vue-2": "^2.0.0-beta.75",
    "@types/marked": "^4.0.1",
    "@types/vue2-editor": "^2.6.1",
    "axios": "^0.24.0",
    "core-js": "^3.15.1",
    "marked": "^4.0.8",
    "node-appwrite": "^4.0.2",
    "nuxt": "^2.15.7",
    "vue2-editor": "^2.10.3",
    "vuex-module-decorators": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.0.7"
  }
