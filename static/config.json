{
  "appwrite": {
    "endpoint": "https://appwrite.raneurope.eu/v1",
    "projectId": "raneurope"
  },
  "theme": {
    "projectName": "RANEurope s.r.o.",
    "projectIcon": "汳ｰ"
  },
  "groups": {},
  "dashboard": {
    "motd": "Vitaj 汨",
    "blocks": [
      {
        "type": "PresentationDivider",
        "config": {
          "text": "Pripravujeme ...",
          "icon": "沁"
        }
      }
    ]
  },
  "panels": {
    "blog": {
      "description": "Vytvorenie novﾃｩho ﾄ考ﾃ｡nku na blog",
      "icon": "沒",
      "name": "Blog",
      "searchAttribute": "title",
      "labels": {
        "only-published": {
          "sorts": [],
          "queries": ["isPublish.equal(true)"],
          "name": "Only published"
        },
        "only-drafts": {
          "sorts": [],
          "queries": ["isPublish.equal(false)"],
          "name": "Only drafts"
        }
      },
      "actions": {
        "edit": {
          "blocksReflectCreate": true
        },
        "list": {
          "blocks": [
            {
              "attribute": "title",
              "name": "Nﾃ｡dpis",
              "width": "300px",
              "type": "ListStringValue"
            },
            {
              "attribute": "isPublish",
              "name": "Publikovanﾃｩ?",
              "type": "ListBooleanValue",
              "width": "80px"
            },
            {
              "attribute": "authorId",
              "name": "Autor",
              "type": "ListRelationValue",
              "width": "200px",
              "config": {
                "collection": "authors",
                "attribute": "name"
              }
            },
            {
              "attribute": "dateCreat",
              "name": "Dﾃ｡tum zverejnenia",
              "type": "ListUnixTimestampValue",
              "width": "150px"
            }
          ]
        },
        "create": {
          "blocks": [
            {
              "attribute": "title",
              "type": "FormTextInput",
              "name": "Nﾃ｡dpis ﾄ考ﾃ｡nku",
              "config": {
                "placeholder": "Sem napﾃｭﾅ｡ nﾃ｡dpis ﾄ考ﾃ｡nku"
              }
            },
            {
              "attribute": "imageId",
              "type": "FormImageInput",
              "name": "Titulnﾃｽ obrﾃ｡zok"
            },
            {
              "attribute": "content",
              "type": "FormRichTextInput",
              "name": "Obsah ﾄ考ﾃ｡nku"
            },
            {
              "attribute": "tags",
              "array": true,
              "type": "FormTextInput",
              "name": "Tagy",
              "config": {
                "placeholder": "Sem napﾃｭﾅ｡ tagy ktorﾃｩ chceﾅ｡ pouﾅｾiﾅ･"
              }
            },
            {
              "attribute": "isPublish",
              "type": "FormBooleanInput",
              "name": "Verejne/Neverejne"
            },
            {
              "attribute": "dateCreat",
              "type": "FormUnixTimestampInput",
              "name": "Dﾃ｡tum zverejnenia"
            },
            {
              "attribute": "authorId",
              "type": "FormSelectRelationInput",
              "name": "Vﾃｽber svoje meno ako autor",
              "config": {
                "collection": "authors",
                "attribute": "name"
              }
            }
          ]
        }
      }
    },
    "authors": {
      "description": "Autori ﾄ考ﾃ｡nkov",
      "icon": "汨ｫ",
      "name": "Autori",
      "actions": {
        "view": {
          "blocks": [
            {
              "type": "PresentationDivider",
              "attribute": null,
              "config": {
                "text": "Podrobnosti o autorovi"
              }
            },
            {
              "type": "DetailStringValue",
              "name": "Celﾃｩ Meno",
              "attribute": "name"
            },
            {
              "type": "DetailStringValue",
              "name": "O Autorovi",
              "attribute": "aboutAuthor"
            },
            {
              "type": "DetailImageValue",
              "name": "Obrﾃ｡zok Autora",
              "attribute": "profilePicture"
            }
          ]
        },
        "list": {
          "blocks": [
            {
              "name": "Fotka",
              "type": "ListImageValue",
              "attribute": "profilePicture",
              "width": "85px"
            },
            {
              "name": "Meno",
              "type": "ListStringValue",
              "attribute": "name"
            },
            {
              "name": "O Autorovi",
              "type": "ListStringValue",
              "attribute": "aboutAuthor"
            }
          ]
        },
        "edit": {
          "blocksReflectCreate": true
        },
        "create": {
          "blocks": [
            {
              "type": "FormTextInput",
              "name": "Celﾃｩ meno",
              "attribute": "name",
              "config": {
                "placeholder": "Celﾃｩ meno autora"
              }
            },
            {
              "type": "FormTextareaInput",
              "name": "O autorovi",
              "note": "Krﾃ｡tky opis autora 1-2 vety.",
              "attribute": "aboutAuthor",
              "config": {
                "placeholder": "Ja som ..."
              }
            },
            {
              "type": "FormImageInput",
              "name": "Profilovﾃ｡ fotka",
              "note": "Fotka autora",
              "attribute": "profilePicture"
            }
          ]
        }
      }
    }
  }
}
