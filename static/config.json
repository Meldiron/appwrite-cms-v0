{
  "appwrite": {
    "endpoint": "https://appwrite.raneurope.eu/v1",
    "projectId": "raneurope"
  },
  "theme": {
    "projectName": "RANEurope s.r.o.",
    "projectIcon": "💰"
  },
  "groups": {},
  "dashboard": {
    "motd": "Vitaj 👋",
    "blocks": [
      {
        "type": "PresentationDivider",
        "config": {
          "text": "Pripravujeme ...",
          "icon": "🎆"
        }
      }
    ]
  },
  "panels": {
    "blog": {
      "description": "Vytvorenie nového článku na blog",
      "icon": "📓",
      "name": "Blog",
      "actions": {
        "edit": {
          "blocksReflectCreate": true
        },
        "create": {
          "blocks": [
            {
              "attribute": "title",
              "type": "FormTextInput",
              "name": "Nádpis článku",
              "config": {
                "placeholder": "Sem napíš nádpis článku"
              }
            },
            {
              "attribute": "imageId",
              "type": "FormImageInput",
              "name": "Titulný obrázok"
            },
            {
              "attribute": "tags",
              "array": true,
              "type": "FormTextInput",
              "name": "Tagy",
              "config": {
                "placeholder": "Sem napíš tagy ktoré chceš použiť"
              }
            },
            {
              "attribute": "isPublish",
              "type": "FormBooleanInput",
              "name": "Verejne/Neverejne"
            },
            {
              "attribute": "content",
              "type": "FormRichTextInput",
              "name": "Obsah článku"
            },
            {
              "attribute": "dateCreat"
            }
          ]
        }
      }
    }
  }
}
